{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "esModuleInterop": true,
    "jsx": "react-native",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {
      "@flyerhq/react-native-chat-ui": ["../src"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
